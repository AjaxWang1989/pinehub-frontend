{
  "name": "pinehub-frontend",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "staging": "vue-cli-service build --mode test",
    "lint": "vue-cli-service lint",
    "test": "karma start"
  },
  "dependencies": {
    "awe-dnd": "^0.3.1",
    "axios": "^0.18.0",
    "blueimp-md5": "^2.10.0",
    "cookie-storage": "^3.1.0",
    "echarts": "^4.1.0",
    "element-ui": "^2.4.5",
    "es6-promise": "^4.2.4",
    "js-base64": "^2.4.8",
    "js-md5": "^0.7.3",
    "karma": "^3.1.1",
    "md5": "^2.2.1",
    "object-hash": "^1.3.0",
    "punycode": "^2.1.1",
    "qrcodejs2": "0.0.2",
    "storage-helper": "^1.4.1",
    "strip-ansi": "^4.0.0",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "vue": "^2.5.13",
    "vue-axios": "^2.1.1",
    "vue-echarts-v3": "^1.0.19",
    "vue-resize": "^0.4.4",
    "vue-router": "^3.0.1",
    "vue-scroll": "^2.1.7",
    "vue-slicksort": "^0.1.10",
    "vue-ueditor": "^0.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "@vue/cli-plugin-babel": "^3.0.0-rc.7",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.7",
    "@vue/cli-service": "^3.0.0-rc.7",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-plugin-vue": "^5.0.0-beta.1",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "vue-html5-editor": "^1.1.1"
  },
  "babel": {},
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential",
      "plugin:vue/strongly-recommended",
      "plugin:vue/recommended"
    ],
    "rules": {
      "no-console": "off",
      "arrow-parens": "off",
      "generator-star-spacing":  "off",
      "no-tabs":  "off",
      "no-mixed-spaces-and-tabs":  "off",
      "indent": ["error", 4],
      "vue/html-indent": ["error", 4],
      "vue/script-indent": ["error", 4],
      "no-unused-vars":  "off",
      "no-trailing-spaces":  "off",
      "eqeqeq":  "off",
      "vue/max-attributes-per-line": [0, {
        "singleline": 1,
        "multiline": {
          "allowFirstLine": false
        }
      }]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
